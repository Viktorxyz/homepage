{
	"manifest_version": 3,
	"name": "HomePage",
	"version": "1.0",
	"permissions": [
		"tabs",
		"scripting"
	],
	"host_permissions": [
		"http://www.google.com/s2/*",
		"https://*/favicon.ico",
		"https://icon.horse/icon/*"
	],
	"commands": {
		"new-tab": {
			"suggested_key": {
				"default": "Ctrl+T",
				"linux": "Ctrl+T",
				"mac": "Ctrl+T"
			},
			"description": "Opens new tab"
		}
	},
	"action": {
		"default_title": "Home Page"
	},
	"background": {
		"service_worker": "service_worker.js",
		"type": "module"
	}
}